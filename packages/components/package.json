  "description": "shared components",
  "main": "lib/index.js",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "deploy-storybook": "storybook-to-ghpages",
    "clean": "rm -rf lib && mkdir lib",
    "build": "npm run clean && cross-env NODE_ENV=production babel src/ --out-dir lib --copy-files --no-copy-ignored --ignore src/**/*.stories.js",
    "postbuild": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
    "prepublishOnly": "rm -rf lib && npm run build && npm run deploy-storybook",
    "postpublish": "rm -rf lib/*"
  },
