{
  "name": "@demo-ui-toolkit/components",
  "version": "1.0.0",
  "description": "shared components",
  "main": "lib/index.js",
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "deploy-storybook": "storybook-to-ghpages",
    "clean": "rm -rf lib && mkdir lib",
    "prebuild": "rm -rf build",
    "build": "yarn build:cjs && yarn build:esm && yarn build:es",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel --config-file ./babel.config.js --extensions \".js,.ts,.jsx,.tsx\" ./src --out-dir ./lib --no-copy-ignored --ignore \"**/*.stories.js\"",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel --config-file ./babel.config.js --extensions \".js,.ts,.jsx,.tsx\" ./src --out-dir ./lib/esm --no-copy-ignored --ignore \"**/*.stories.js\"",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel --config-file ./babel.config.js --extensions \".js,.ts,.jsx,.tsx\" ./src --out-dir ./lib/es --no-copy-ignored  --ignore \"**/*.stories.js\"",
    "postbuild": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
    "prepublishOnly": "rm -rf lib && npm run build && npm run deploy-storybook",
    "postpublish": "rm -rf lib/"
  },
  "author": "",
  "files": [
    "lib"
  ],
  "dependencies": {
    "@demo-ui-toolkit/mui-themes": "0.0.12"
  },
  "peerDependencies": {
    "@material-ui/core": "4.9.13",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@demo-ui-toolkit/design-tokens": "^0.0.12",
    "@material-ui/core": "^4.9.13",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-storysource": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@storybook/storybook-deployer": "^2.8.5",
    "babel-loader": "^8.1.0",
    "babel-plugin-optimize-clsx": "^2.6.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^7.0.2",
    "html-loader": "^1.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rollup": "^2.8.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-uglify": "^6.0.4",
    "storybook-addon-material-ui": "^0.9.0-alpha.21"
  },
  "license": "MIT",
  "gitHead": "eb426eeaa6d18cedc4e0b6ea0b959de536fa1fb3"
}
